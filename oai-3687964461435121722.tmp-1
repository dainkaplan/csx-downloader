          <dc:title>Estimation of DeltaSigma converter spectrum</dc:title>
          <dc:creator>Emilia Nunzi</dc:creator>
          <dc:creator>Paolo Carbone</dc:creator>
          <dc:creator>Dario Petri</dc:creator>
          <dc:creator>E. Nunzi</dc:creator>
