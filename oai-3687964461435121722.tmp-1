          <dc:title>Finding Optimal Views for 3D Face Shape Modeling</dc:title>
          <dc:creator>Jinho Lee Baback</dc:creator>
          <dc:creator>Jinho Lee</dc:creator>
          <dc:creator>Baback Moghaddam</dc:creator>
